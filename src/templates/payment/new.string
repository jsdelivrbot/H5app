<header>                <div class="top"><%=base.tradingStatus%></div>                <div class="middle"><h2><%=base.money%></h2>元</div>                <div class="bottom">                    <img class="title" src='"'<%=base.tradingObject.icon%>'"' alt=""/>                    <span><%=base.tradingObject.icon%></span>                </div>             </header>             <% if (orderStatus.isshow == "0") { %>				<section id="start">                    <div class="all">                        <% var status = orderStatus.list[0].status;                          var operationTime = orderStatus.list[0].operationTime;                          var statusName = orderStatus.list[0].statusName; %>                    <% for (var i=1;i<orderStatus.list.length;i++){                           var status = orderStatus.list[i].status;                           var operationTime = orderStatus.list[i].operationTime;                           var statusName = orderStatus.list[i].statusName;%>                        <div class="step">                        <%if(orderStatus.list[i].current == '0'){                            if(i == 0){%>                                <div class="step">                                    <span class="circle current"></span>                                    <h3 id="submit"><%=statusName%></h3>                                    <p><%=operationTime%></p>                                </div>                            <%}else%>                            <hr class="current"/>                            <span class="circle current"></span>                            <h3 class="current"><%=statusName%></h3>                            <p><%=operationTime%></p>                        <%}else{%>                            <hr/>                            <span class="circle"></span>                            <h3 id="submit"><%=statusName%></h3>                             <p><%=operationTime%></p>                        <%}%>                     </div>                     <% } %>                    </div>                    <ul>                        <% for (var i = 0;i<orderDetail.length;i++){                            var item = orderDetail[i].key;                            var value = orderDetail[i].val%>                            <li><span class="item"><%=item%></span><%=value%></li>                        <% } %>                    </ul>                </section>			<% } %>			<% else if (orderStatus.isshow == "1") { %>				<section id="end">                    <ul>                    <% for (var i=0)%>                        <li><span class="item">订单金额</span><%=base.money%></li>                        <li><span class="item">优惠金额</span>－5.00元</li>                        <li><span class="item">优惠信息</span>优惠券5.00元</li>                        <li id="middle" ><span class="item"></span>借贷宝优惠0.00元</li>                        <li><span class="item">当前状态</span>已退款</li>                        <li><span class="item">商品名称</span>梦龙冰淇凌－巧克力味＊8</li>                        <li><span class="item">支付方式</span>借贷宝余额</li>                        <li><span class="item">消费时间</span>2015-09-03 19:32:32</li>                        <li><span class="item">订单编号</span>201509051932323</li>                    </ul>                </section>			<% } %>